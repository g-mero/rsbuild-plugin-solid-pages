{
  "name": "rsbuild-plugin-solid-pages",
  "type": "module",
  "version": "0.1.1",
  "description": "rsbuild-plugin-solid-pages",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./client": {
      "types": "./dist/client.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {},
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "pub": "pnpm build && pnpm publish -r",
    "test": "vitest"
  },
  "peerDependencies": {
    "@rsbuild/core": "1.x",
    "@solidjs/router": "^0.15.2"
  },
  "peerDependenciesMeta": {
    "@rsbuild/core": {
      "optional": true
    },
    "@solidjs/router": {
      "optional": true
    }
  },
  "dependencies": {
    "@swc/core": "^1.10.3",
    "@types/fs-extra": "^11.0.4",
    "es-module-lexer": "^1.5.4",
    "esbuild": "^0.24.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "gray-matter": "^4.0.3",
    "radash": "^12.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3",
    "@repo/ts-config": "workspace:*",
    "@types/node": "^22.5.2",
    "eslint": "^9",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vite": "^6.0.6",
    "vitest": "^2.1.8"
  },
  "browser": {
    "./dist/server.js": "./dist/index.js"
  }
}
